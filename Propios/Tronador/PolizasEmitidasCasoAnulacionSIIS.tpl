SELECT S.NRO_SOAT,S.* 
FROM SIM_DATOSSOAT S 
INNER JOIN SIM_LOG_FASESIIS_SOAT F
ON S.NUM_SECU_POL=F.NUM_SECU_POL
AND S.NRO_SOAT=F.NUM_POL1 
WHERE FECHA_CREACION>='01/07/2023' 
AND NRO_SOAT IS NOT NULL 
AND F.RADICADO IS NOT NULL;


